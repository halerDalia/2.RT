#include <iostream>
#include <stdio.h>
using namespace std;


float Kruzniluk(int r, int a)  // izracunaj duljinu kruznog luka ako je zadani r
{
	float l;
	l = ((r * 3.14 * a) / 180);
		return l;

}

float Kruzniisjecak(int r, int a)   // funkcija koja vrati povrsinu kruznog isjecka 
{
	float p;
	p = ((r * r * 3.14 * a) / 180);
	return p;


}


void abeceda(char p, char z) // funkcija koja ispise sva mala slova engleske abecede od pocetnog do zavrsnog slova
{
	char a;
	char t;
	for (a = p; a != z; a++)
		if (t > 'z')
			cout << "z";


}


int Vrijeme(int sec) // izracunaj sate, imute i sekunde ako su zadane samo sekunde
{
	int h;
	int min;
	int s;
	h = sec / 3600;
	sec %= 3600;
	min = sec / 60;
	s = sec % 60;
	return h;

}



